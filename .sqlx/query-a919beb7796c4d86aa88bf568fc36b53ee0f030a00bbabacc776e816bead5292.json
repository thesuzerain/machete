  "query": "\n        SELECT \n            ch.id,\n            ch.name,\n            ch.player,\n            ch.level,\n            ch.class\n        FROM characters ch\n        LEFT JOIN campaigns ca ON ch.campaign = ca.id\n        WHERE \n            ($1::text IS NULL OR ch.name ILIKE '%' || $1 || '%')\n            AND ($2::int IS NULL OR ca.id = $2)\n    ",
